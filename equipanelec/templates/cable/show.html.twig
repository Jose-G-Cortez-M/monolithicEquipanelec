    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cable.id }}</td>
            </tr>
            <tr>
                <th>Barcode</th>
                <td>{{ cable.barcode }}</td>
            </tr>
            <tr>
