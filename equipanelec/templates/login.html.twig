<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
</head>

<body>

<div class="container">
    <div class="table-responsive">
        <div class="col-12">
            {% block body %}{% endblock %}
        </div>
    </div>
</div>
<div style="background: #FFFFFF; width: 100%;height: 300px"></div>
<div id="pie-footer">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <p class="titulo-footer">
                    Copyright &copy; All right reserved | This template mode with <span class="span-subtitulo"> By ColorLib</span>
                </p>

            </div>
        </div>
    </div>
</div>


{% block javascripts %}
    {{ encore_entry_script_tags('app') }}
{% endblock %}

</body>
</html>